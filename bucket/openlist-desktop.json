{
    "version": "0.7.0",
    "description": "A desktop application for OpenList",
    "homepage": "https://github.com/OpenListTeam/OpenList-Desktop",
    "license": "GPL-3.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://github.com/OpenListTeam/OpenList-Desktop/releases/download/v0.7.0/OpenList.Desktop_0.7.0_x64-setup.exe#/dl.7z",
            "hash": "b9ce745d161c1b81c4542adc1a3ad0ec2963f0e0572bbfbdf0c707c6973c8b96"
        },
        "arm64": {
            "url": "https://github.com/OpenListTeam/OpenList-Desktop/releases/download/v0.7.0/OpenList.Desktop_0.7.0_arm64-setup.exe#/dl.7z",
            "hash": "493c4f40895a520883f175771fdecdffcfaa280a1e653b16f39acfbedc7ebff9"
        }
    },
    "pre_install": [
        "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Recurse",
        "New-Item -ItemType Directory -Path \"$persist_dir\" -Force | Out-Null",
        "if (!(Test-Path \"$persist_dir\\settings.json\") -or !(Get-Item \"$persist_dir\\settings.json\").Length) {",
        "    \"{}\" | Out-File -FilePath \"$persist_dir\\settings.json\" -Encoding utf8 -Force",
        "}",
        "if (!(Test-Path \"$persist_dir\\rclone.conf\")) {",
        "    New-Item -ItemType File -Path \"$persist_dir\\rclone.conf\" | Out-Null",
        "}",
        "New-Item -ItemType SymbolicLink -Path \"$dir\\rclone.conf\" -Target \"$persist_dir\\rclone.conf\" | Out-Null"
    ],
    "persist": [
        "data",
        "logs",
        "settings.json"
    ],
    "shortcuts": [
        [
            "openlist-desktop.exe",
            "OpenList Desktop"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/OpenListTeam/OpenList-Desktop/releases/download/v$version/OpenList.Desktop_$version_x64-setup.exe#/dl.7z"
            },
            "arm64": {
                "url": "https://github.com/OpenListTeam/OpenList-Desktop/releases/download/v$version/OpenList.Desktop_$version_arm64-setup.exe#/dl.7z"
            }
        }
    }
}
